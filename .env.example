# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Domain Configuration
EXPO_PUBLIC_WEB_URL=https://civicspark.app
EXPO_PUBLIC_API_URL=https://civicspark.app/api

# Optional: For social authentication
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id
EXPO_PUBLIC_APPLE_CLIENT_ID=your_apple_client_id

# Development
EXPO_PUBLIC_ENVIRONMENT=production

# Debug Panel Control (set to 'true' to show debug panel, 'false' or omit to hide)
EXPO_PUBLIC_SHOW_DEBUG_PANEL=false